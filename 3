#include <bits/stdc++.h>
#include <utility>
using namespace std;

int main() {
	// your code goes here
	int x=1;
	int t; cin>>t;
	while(t--){
	    int n; cin>>n;
	    pair<int,int> a[n];int max1=INT_MIN;
	    for(int i=0;i<n;i++) {
	        cin>>a[i].first>>a[i].second;
	        if(max1<a[i].first) max1=a[i].first;
	    }
	    pair<int,int> p[n]=a;
	    char ch[max1+1];
	    sort(p,p+n);
	    bool is=true;
	    cout<<"case #"<<x++<<": ";
	    for(int i=2;i<n;i++)
	        if(p[i].first < p[i-1].second && p[i].first < p[i-2].second)
	        {
	            cout<<"IMPOSSIBLE";
	            is=false;
	            break;
	        }
	        if(is){
	             ch[p[0].first]='J';
	             ch[p[1].first]='C';
	             for(int i=2;i<n;i++){
	             if(p[i].first>=p[i-2].second ){
	              ch[p[i].first]='J';
	              p[i-2]=p[i-1];
	              p[i-1]=p[i];
	              p[i]=p[i-2];
	            } 
	            else if(p[i].first>=p[i-1].second) {
	                ch[p[i].first]='C';
	                p[i-1]=p[i-2];
	            }
	             }
	    for(int i=0;i<n;i++) cout<<ch[a[i].first];
	        }
	    cout<<endl;
	}
	return 0;
}
